    "    w_gd=[]\n",
    "    w_gd.append(w_start)\n",
    "    pre_w=w_start\n",
    "\n",
    "    for i in range(epochs):\n",
    "        w=pre_w-lr*df(pre_w)\n",
    "        w_gd.append(w)\n",
    "        pre_w=wreturn np.array(w_gd)"
